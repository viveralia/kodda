{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "~api": ["src/api"],
      "~components": ["src/components"],
      "~features/*": ["src/features/*"],
      "~hooks/*": ["src/hooks/*"],
      "~navigators/*": ["src/navigators/*"],
      "~theme": ["src/theme"],
      "~utils/*": ["src/utils/*"]
    }
  }
}
